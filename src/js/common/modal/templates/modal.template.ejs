<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="js-modal-close close" aria-label="Close"><span aria-hidden="true">&times;</span></button>

        <h4 class="modal-title" id="modalLabel"><%- title %></h4>
    </div>

    <div class="modal-body"></div>

    <div class="modal-footer">
    	<button type="submit" class="js-modal-submit btn btn-primary"><%- submitButtonText %></button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><%- dismissButtonText %></button>
    </div>
</div>
